{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory 이름",
			"defaultValue": "sptigerADFtraining"
		},
		"AzureDatabricksDeltaLake1_accessToken": {
			"type": "secureString",
			"metadata": "'AzureDatabricksDeltaLake1'의 'accessToken'에 대한 보안 문자열"
		},
		"GoogleBigQueryTest_keyFileContent": {
			"type": "secureString",
			"metadata": "'GoogleBigQueryTest'의 'keyFileContent'에 대한 보안 문자열"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "'GitHub1'의 'credential'에 대한 보안 문자열"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "chunho-kim"
		},
		"containerUri": {
			"type": "string",
			"metadata": "스토리지 계정 연결 된 ARM 서식 파일을 포함 하는 URI입니다."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "연결된 ARM 템플릿을 포함하는 스토리지 계정에 대한 SAS 토큰"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sptigerADFtraining_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksDeltaLake1_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake1_accessToken')]"
					},
					"GoogleBigQueryTest_keyFileContent": {
						"value": "[parameters('GoogleBigQueryTest_keyFileContent')]"
					},
					"GitHub1_credential": {
						"value": "[parameters('GitHub1_credential')]"
					},
					"GitHub1_properties_typeProperties_username": {
						"value": "[parameters('GitHub1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}